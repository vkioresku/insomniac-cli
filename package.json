{
  "name": "@vkioresku/insomniac-cli",
  "version": "1.0.6",
  "description": "A CLI to easily create React components with files such as styles and type declarations",
  "main": "src/index.js",
  "bin": {
    "insomniac-cli": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm pack && npm link",
    "tsc": "tsc -b",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --fix --ext .ts",
    "sync-templates": "cp -r src/Templates lib/Templates"
  },
  "keywords": [
    "insomniac",
    "cli"
  ],
  "author": "Vladislav Chiorescu",
  "license": "MIT",
  "dependencies": {
    "@types/mustache": "^4.0.1",
    "chalk": "^4.1.0",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "log-symbols": "^4.0.0",
    "mustache": "^4.0.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/figlet": "^1.2.1",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "types": "lib/cli.d.ts",
  "files": [
    "/bin",
    "/lib"
  ]
}
